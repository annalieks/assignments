<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/MainFragment">

    <fragment
        android:id="@+id/MainFragment"
        android:name="com.example.equationcalculator.MainFragment"
        android:label="@string/project_label"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_MainFragment_to_CalculatorFragment"
            app:destination="@id/CalculatorFragment" />

        <action
            android:id="@+id/action_MainFragment_to_DbGpsFragment"
            app:destination="@id/DbGpsFragment" />
    </fragment>

    <fragment
        android:id="@+id/CalculatorFragment"
        android:name="com.example.equationcalculator.CalculatorFragment"
        android:label="@string/calculator_label"
        tools:layout="@layout/fragment_calculator">
    </fragment>

    <fragment
        android:id="@+id/DbGpsFragment"
        android:name="com.example.equationcalculator.DbGpsFragment"
        android:label="@string/dbgps_label"
        tools:layout="@layout/fragment_db_gps">

        <action
            android:id="@+id/action_DbGpsFragment_to_MainFragment"
            app:destination="@id/MainFragment" />

    </fragment>
</navigation>